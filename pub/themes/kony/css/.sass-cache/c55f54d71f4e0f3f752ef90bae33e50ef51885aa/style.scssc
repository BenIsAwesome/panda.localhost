.clearfix {
    height: 0 !important;
    margin-bottom: -17px;
}

/* RWD phone fixes */
@media only screen and (max-width: 500px) {

    #utility-bar select {
      display: none;
    }

    .feedback-link {
    	display: none;
    }

    div.resource.three, div.resource.four, div.col.six.left, .kony-feature-next-steps .col {
     	width: 100%; 
    }

    div.partner.three {
    	width: 50%; 
    }

    div.partner.three p, div.partner.three img {
    	width: 90%; 
    }
    
    .kony-feature-tabbed-comparison {
        display: none;
    }
    
    #connect {
        width: 100%;
        border-right: 0; 
    }
    
    #connect h4 {
       text-align: center; 
    }

    #connect li a {
        text-indent: -10000px;
    }
    
    #footer-links {
        width: 100%;
        padding-left: 0 !important;
    }
    
    #footer-links li {
        text-align: center;
        width: 100%;
    }
    
    #footer-links+h4 {
        clear: both;
        display: block;
        padding-top: 50px;
    }
    
    #footer-logo {
      margin-right: 21%;
    }
    
    .kony-feature-value-prop figure {
        margin-top: 42px;
        border-top: 1px solid white;
    }
    
    #posts {
        width: 100%;
    }
    
    aside {
        display: none;
    }
    
    .two-tabs li, .three-tabs li, .four-tabs li, .five-tabs li, .six-tabs li {
        width: 100% !important;
    }
    
    div.col.five.left, div.col.seven.left {
        width: 100%;
        clear: both;
    }
    
    .app-headline a {
        display: block;
        clear: both;
        margin: 0 0 20px 0;
        width: 100% !important;
    }
    
    .app-listing {
        width: 50%;
    }
    
    .app-listing h5 {
        margin: 10px 0;
    }
    
    h2 {
        font-size: 230% !important;
        margin: -10px 0 15px 0 !important;    
    }
    
    /* Leadership fixed */
    .col.six, .col.five, .leader, .leader img {
      width: 100% !important;
    }
    
   
}



